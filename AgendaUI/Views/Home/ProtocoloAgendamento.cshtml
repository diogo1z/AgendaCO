@model AgendaUI.Models.ProtocoloVM

<div id="wrapper" class="container-full" style="min-height:calc(100vh - 150px); margin:0; box-sizing:border-box;">
    <!-- Sidebar -->
    <div id="sidebar-wrapper" style="margin-top: 50px;">
        <ul class="sidebar-nav">
            <li>
                Selecione a clínica
            </li>
            <li>
                Selecione a Data e Horário
            </li>
            <li>
                Usuário
            </li>
            <li>
                Paciente
            </li>
        </ul>
    </div>
    <!-- /#sidebar-wrapper -->
    <!-- Page Content -->
    @Html.AntiForgeryToken()
    <div id="page-content-wrapper">

        <div class="row ">
            <div class="col-lg-12">
                <h1>Parabéns! Seu agendamento foi realizado com sucesso!</h1>
            </div>
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(x => x.Protocolo)
            @Html.ValueFor(x => x.Protocolo)
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(x => x.Clinica)
            @Html.ValueFor(x => x.Clinica)
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(x => x.DataAgendamento)
            @Html.ValueFor(x => x.DataAgendamento)
        </div>

        <div class="form-group col-md-12">
            @Html.LabelFor(x => x.HoraAgendamento)
            @Html.ValueFor(x => x.HoraAgendamento)
        </div>
        <div class="form-group col-md-12">
            <label>Agendamento realizado as @DateTime.Now</label>            
        </div>
    </div>
    <!-- /#page-content-wrapper -->
</div>